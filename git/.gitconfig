[alias]
    br = branch -av
    co = checkout
    st = status
[apply]
    # Detect whitespace errors when applying a patch
    whitespace = fix
[color]
    # Use color where available
    ui = auto
[core]
    editor = vim
	excludesfile = /Users/arnav/.gitignore
[help]
    # Automatically correct and execute typos
    autocorrect = 1
[push]
    # Use the git 2.0 behavior
    default = simple
[user]
